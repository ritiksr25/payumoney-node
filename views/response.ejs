<% include ./partials/head %>
<h1>Transaction Details!!</h1>
<div class="row">
    <div class="col-lg-6 mx-auto">
        <div class="card mt-4 mx-auto">
            <ul class="list-group">
                <li class="list-group-item">
                    <strong>
                        Transaction ID: <%=data.txnid%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        Status: <%=data.status%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        Name: <%=data.firstname%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        email: <%=data.email%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        Amount: <%=data.amount%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        PayUMoney ID: <%=data.payuMoneyId%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        Payment Mode: <%=data.mode%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        Date: <%=data.addedon%></strong>
                </li>
                <li class="list-group-item">
                    <strong>
                        Bank Reference: <%=data.bank_ref_num%></strong>
                </li>
            </ul>
            <a href="/" class="btn btn-primary mt-2" title="Home">Home</a>
        </div>
    </div>
</div>

<% include ./partials/footer %>